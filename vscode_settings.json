{
  "[markdown]": {
    "editor.wordWrap": "off"
  },

  "[powershell]": {
    "#": "Disable 'n references' atop of functions",
    "editor.codeLens": false
  },

  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "html"
  },

  "vim.leader": "<space>",
  "vim.ignorecase": true,
  "vim.insertModeKeyBindings": [{
    "before": ["<c-v>"],
    "after": [],
    "commands": [{
        "command": "editor.action.clipboardPasteAction",
        "args": []
    }]
  }],
  "vim.normalModeKeyBindingsNonRecursive": [{
    "before": ["n"],
    "after": ["n", "z", "z"]
  },
  {
    "before": ["N"],
    "after": ["N", "z", "z"]
  }],
  "vim.otherModesKeyBindingsNonRecursive": [{
    "before": ["Z", "Z"],
    "after": [],
    "commands": [{
      "command": "workbench.action.closeActiveEditor",
      "args": []
    }]
  },
  {
    "before": ["<c-o>"],
    "after": [],
    "commands": [{
        "command": "workbench.action.files.openFile",
        "args": []
    }]
  },
  {
    "before": ["<c-w>"],
    "after": [],
    "commands": [{
        "command": "workbench.action.closeActiveEditor",
        "args": []
    }]
  },
  {
    "before": ["<c-]>"],
    "after": [],
    "commands": [{
        "command": "editor.action.openLink",
        "args": []
    }]
  },
  {
    "before": ["<c-[>"],
    "after": [],
    "commands": [{
        "command": "workbench.action.navigateBack",
        "args": []
    }]
  },
  {
    "before": ["<leader>", "p"],
    "after": [],
    "commands": [{
        "command": "workbench.action.showCommandPalette",
        "args": []
    }]
  }],

  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": {
      "lint": true
    }
  },

  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "[python]": {
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "editor.lineNumbers": "off",
  "editor.links": true,
  "editor.minimap.enabled": false,
  "editor.autoClosingBrackets": "never",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "line",
  "editor.rulers": [78],
  "editor.glyphMargin": true,
  "editor.wordWrap": "off",
  "editor.autoClosingQuotes": "never",
  "editor.fontFamily": "JetBrainsMono Nerd Font, JetBrainsMono NF, JetBrainsMono",
  "editor.semanticHighlighting.enabled": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, JetBrainsMono NF, JetBrainsMono",
  "editor.fontSize": 16,
  "terminal.integrated.fontSize": 16,
  "editor.fontLigatures": true,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 0,
  "window.openFilesInNewWindow": "off",
  "window.openFoldersInNewWindow": "off",
  "window.newWindowDimensions": "maximized",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 0,
  "git.confirmSync": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "extensions.ignoreRecommendations": false,
  "editor.renderControlCharacters": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.editor.labelFormat": "short",
  "workbench.activityBar.visible": true,
  "workbench.editor.highlightModifiedTabs": true,
  "npm.enableScriptExplorer": true,
  "breadcrumbs.enabled": true,
  "powershell.integratedConsole.showOnStartup": false,
  "html.autoClosingTags": false,
  "files.eol": "\n",
  "telemetry.enableTelemetry": false,
  "workbench.colorTheme": "grigoryvp memory theme",
  "debug.node.autoAttach": "disabled",
  "terminal.integrated.shell.windows": "pwsh.exe",
  "workbench.iconTheme": "vs-seti",
  "scm.diffDecorations": "none",
  "vsicons.presets.foldersAllDefaultIcon": true,
  "vsicons.dontShowNewVersionMessage": true,
  "keyboard.dispatch": "keyCode",
  "python.languageServer": "Pylance"
}
